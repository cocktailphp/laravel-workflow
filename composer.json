{
    "name": "brexis/laravel-workflow",
    "description": "Integerate Symfony Workflow component into Laravel.",
    "keywords": ["workflow", "symfony", "laravel", "laravel5"],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/workflow": "^3.2",
        "symfony/process": "^3.1",
        "symfony/event-dispatcher": "^3.1",
        "illuminate/console": "5.3.* | 5.4.* | 5.5.*",
        "illuminate/support": "5.3.* | 5.4.* | 5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Brexis\\LaravelWorkflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "mockery/mockery": "^0.9.8",
        "phpunit/phpunit": "^6.0"
    }
}
